{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ReverseProxy": {
    "Routes": [
      {
        "RouteId": "ServerRoute",
        "ClusterId": "Server",
        "Match": {
          "Path": "/api/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/api"
          }
        ]
      },
      {
        "RouteId": "ServerSwaggerRoute",
        "ClusterId": "Server",
        "Match": {
          "Path": "/swagger/{**catch-all}"
        }
      },
      {
        "RouteId": "ClientRoute",
        "ClusterId": "Client",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    ],
    "Clusters": {
      "Client": {
        "Destinations": {
          "Client": {
            "Address": "https://localhost:5001"
          }
        }
      },
      "Server": {
        "Destinations": {
          "Server": {
            "Address": "https://localhost:5003"
          }
        }
      }
    }
  }
}